<p>Submit new photo</p>
 <%= bootstrap_flash %>
<%= form_for @submission, url: submissions_path, html: { multipart: true}  do |f| %>
  <%= f.label :photo %>
  <%= f.file_field :photo %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :tag_names, 'Tags' %>
  <%= f.text_field :tag_names %>

  <%= f.submit 'Save photo' %>

<% end %>